    "express": "^4.17.1",
    "ffmpeg-static": "^4.4.0",
    "jest": "^27.2.5",
    "opusscript": "^0.0.8",
    "readline": "^1.3.0",
    "ytdl-core": "^4.9.1"
  },
  "devDependencies": {
    "@types/colors": "^1.2.1",
    "@types/dateformat": "^3.0.1",
    "@types/ejs": "^3.1.0",
    "@types/eslint": "^7.28.1",
    "@types/express": "^4.17.13",
    "@types/ffmpeg-static": "^3.0.0",
    "@types/jest": "^27.0.2",
    "@types/prompt-sync": "^4.1.1",
